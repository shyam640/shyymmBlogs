{
  "name": "shyymm-blogs",
  "version": "1.0.0",
  "description": "This is my portfolio and blog site build on MERN stack",
  "main": "index.js",
  "scripts": {
    "start": "nodemon backend/index.js",
    "client": "npm start --prefix frontend",
    "dev": "concurrently \"npm start \" \"npm run client \"",
    "build": "CI=false && cd frontend && npm install --legacy-peer-deps && npm run build ",
    "test": "jest --verbose --runInBand",
    "test:frontend": "npm test --prefix frontend",
    "test:backend": "jest --verbose --runInBand --testPathPattern backend/__tests__",
    "test:all": "concurrently \"npm run test:backend\" \"npm run test:frontend\""
  },
  "author": "its_shyam640",
  "license": "ISC",
  "dependencies": {
    "@material-ui/core": "^4.12.4",
    "@material-ui/icons": "^4.11.3",
    "bcrypt": "^5.1.0",
    "buffer": "^6.0.3",
    "cloudinary-react": "^1.8.1",
    "concurrently": "^8.2.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "jsonwebtoken": "^9.0.1",
    "material-ui-dropzone": "^3.5.0",
    "mongoose": "^7.4.0",
    "multer": "^1.4.5-lts.1",
    "nodemon": "^3.0.1",
    "path": "^0.12.7",
    "react-bootstrap": "^2.8.0",
    "react-dropzone": "^14.2.3",
    "react-helmet": "^6.1.0",
    "react-redux": "^8.1.1",
    "redux": "^4.2.1",
    "redux-thunk": "^2.4.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.9",
    "@babel/core": "^7.22.9",
    "@babel/preset-env": "^7.22.9",
    "@babel/preset-react": "^7.22.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "jest": "^29.6.1",
    "jest-puppeteer": "^9.0.0",
    "supertest": "^6.3.3"
  }
}
